<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Crossborwser" parallel="tests">
  <test thread-count="5" name="CrossbrowserChrome">
	  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="testScript.PayoutTest"/>
    </classes>
  </test> <!-- Crossbrowser -->
  
  <test thread-count="5" name="CrossbrowserEdge">
	  <parameter name="browser" value="edge"></parameter>
    <classes>
      <class name="testScript.PayoutTest"/>
    </classes>
  </test> <!-- Crossbrowser -->
  
  <test thread-count="5" name="CrossbrowserFirefox" enabled="false">
	  <parameter name="browser" value="firefox"></parameter>
    <classes>
      <class name="testScript.PayoutTest"/>
    </classes>
  </test> <!-- Crossbrowser -->
  
</suite> <!-- Crossborwser -->
